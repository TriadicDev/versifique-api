{
  "name": "versifique-api",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "env:dev": "cp ./environments/.env.dev ./.env",
    "env:prod": "cp ./environments/.env.prod ./.env",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r dotenv/config .",
    "start:dev": "npm run env:dev && npm start",
    "start:prod": "npm run env:prod && npm start",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Bruna Suelen <bruna.srgs@gmail.com>, Felipe Silva Rezende <feliperezende.srz@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^3.4.2",
    "@loopback/core": "^2.16.2",
    "@loopback/repository": "^3.7.1",
    "@loopback/rest": "^10.0.0",
    "@loopback/rest-explorer": "^3.3.2",
    "@loopback/service-proxy": "^3.2.2",
    "dotenv": "^10.0.0",
    "install": "^0.13.0",
    "loopback-connector-mongodb": "^6.0.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "source-map-support": "^0.5.19",
    "@loopback/testlab": "^3.4.2",
    "@types/node": "^10.17.60",
    "@loopback/eslint-config": "^11.0.0",
    "eslint": "^7.29.0",
    "typescript": "~4.3.4"
  }
}
